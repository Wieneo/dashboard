+        const user = req.user
+        const namespace = req.params.namespace
+        const name = req.params.name
+        const body = req.body
+        res.send(await shoots.createAdminKubeconfig({ user, namespace, name, body }))
+      } catch (err) {
+        next(err)
+      }
+    })
